@model DolphinFx.Models.Client

<dl class="row">
    <dt class="col-sm-6">Client Name</dt>
    <dd class="col-sm-6">@Html.DisplayFor(model => model.ClientName)</dd>
    <dt class="col-sm-6">Primary Contact</dt>
    <dd class="col-sm-6">@Html.DisplayFor(model => model.PrimaryContact)</dd>
    <dt class="col-sm-6">Primary EmailID</dt>
    <dd class="col-sm-6">@Html.DisplayFor(model => model.PrimaryEmailID)</dd>
    <dt class="col-sm-6">Secondary Contact</dt>
    <dd class="col-sm-6">@Html.DisplayFor(model => model.SecondaryContact)</dd>
    <dt class="col-sm-6">Secondary EmailID</dt>
    <dd class="col-sm-6">@Html.DisplayFor(model => model.SecondaryEmailID)</dd>
</dl>